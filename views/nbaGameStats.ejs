<div class="row">
   <div class="col-md-9">
      <h2><%= obj['game']['team'][0]['$'].name %> Player Stats</h2>
      <div class='table-responsive'>
      <table class='table table-striped table-hover table-bordered'>
         <tr>
            <td>Name</td>
            <td >pos</td>
            <td >min</td>
            <td >fgm-a</td>
            <td >3pm-a</td>
            <td >ftm-a</td>
            <td >+/-</td>
            <td >off</td>
            <td >def</td>
            <td >tot</td>
            <td >ast</td>
            <td >pf</td>
            <td >st</td>
            <td >to</td>
            <td >bs</td>
            <td >pts</td>
         </tr>
         <% for (var i = 0; i<obj['game']['team'][0]['players'][0]['player'].length; i++) { %>
         <tr>
            <td><%=obj['game']['team'][0]['players'][0]['player'][i]['$'].full_name%> </td>
            <td><%=obj['game']['team'][0]['players'][0]['player'][i]['$'].primary_position%> </td>
            <% if(obj['game']['team'][0]['players'][0]['player'][i]['statistics'][0]['$'].minutes != undefined) { %>
            <td><%=obj['game']['team'][0]['players'][0]['player'][i]['statistics'][0]['$'].minutes%> </td>
            <% } else { %>
            <td>DNP</td>
            <% } %>
            <td><%=obj['game']['team'][0]['players'][0]['player'][i]['statistics'][0]['$'].field_goals_made %> - <%= obj['game']['team'][0]['players'][0]['player'][i]['statistics'][0]['$'].field_goals_att %></td>
            <td><%=obj['game']['team'][0]['players'][0]['player'][i]['statistics'][0]['$'].three_points_made %> - <%= obj['game']['team'][0]['players'][0]['player'][i]['statistics'][0]['$'].three_points_att %></td>
            <td><%=obj['game']['team'][0]['players'][0]['player'][i]['statistics'][0]['$'].free_throws_made %> - <%= obj['game']['team'][0]['players'][0]['player'][i]['statistics'][0]['$'].free_throws_att %></td>
            <% if(obj['game']['team'][0]['players'][0]['player'][i]['statistics'][0]['$'].pls_min != undefined) { %>
            <td><%=obj['game']['team'][0]['players'][0]['player'][i]['statistics'][0]['$'].pls_min%> </td>
            <% } else { %>
            <td>N/A</td>
            <% } %>
            <td><%=obj['game']['team'][0]['players'][0]['player'][i]['statistics'][0]['$'].offensive_rebounds %></td>
            <td><%=obj['game']['team'][0]['players'][0]['player'][i]['statistics'][0]['$'].defensive_rebounds %></td>
            <td><%=obj['game']['team'][0]['players'][0]['player'][i]['statistics'][0]['$'].rebounds %></td>
            <td><%=obj['game']['team'][0]['players'][0]['player'][i]['statistics'][0]['$'].assists %></td>
            <td><%=obj['game']['team'][0]['players'][0]['player'][i]['statistics'][0]['$'].personal_fouls %></td>
            <td><%=obj['game']['team'][0]['players'][0]['player'][i]['statistics'][0]['$'].steals %></td>
            <td><%=obj['game']['team'][0]['players'][0]['player'][i]['statistics'][0]['$'].turnovers %></td>
            <td><%=obj['game']['team'][0]['players'][0]['player'][i]['statistics'][0]['$'].blocks %></td>
            <td><%=obj['game']['team'][0]['players'][0]['player'][i]['statistics'][0]['$'].points %></td>
         </tr>
         <% } %>
      </table>
      </div>
   </div>
</div>
