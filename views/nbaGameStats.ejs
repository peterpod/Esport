<div class="jumbotron">
   <div class="container">
    <h1>NBA Game Statistics</h1>
    <h2><%= obj['game']['team'][0]['$'].name %> Versus <%= obj['game']['team'][1]['$'].name %> </h2>
    <h2>FINAL</h2>
    <table width="151" cellpadding="0" cellspacing="0">
      <tr>
         <td class="nbaGIATeam"><%= obj['game']['team'][0]['$'].name%> </td>
         <td class="nbaGIAScr"><%=obj['game']['team'][0]['$'].points%></td>
      </tr>
      <tr>
         <td class="nbaGIHTeam"><%=obj['game']['team'][1]['$'].name%> </td>
         <td class="nbaGIHScr"> <%=obj['game']['team'][1]['$'].points%></td>
      </tr>
   </table>
   <div class='table-responsibe'>
         <table class='table'>
            <tr>
               <td>Quarter 1</td>
               <td>Quarter 2</td>
               <td>Quarter 3</td>
               <td>Quarter 4</td>
               <td>Final</td>
            </tr>
            <tr>
               <% if(obj['game']['team'][0]['scoring'][0]['quarter'][0] !== undefined ) { %>
                  <td class="nbaGIQtrPts"><%=obj['game']['team'][0]['scoring'][0]['quarter'][0]['$'].points%> </td>
               <% } else { %>
                  <td>0</td>
               <% } %>
               <% if(obj['game']['team'][0]['scoring'][0]['quarter'][1] !== undefined) { %>
                  <td class="nbaGIQtrPts"><%=obj['game']['team'][0]['scoring'][0]['quarter'][1]['$'].points%> </td>
               <% } else { %>
                  <td>0</td>
               <% } %>
               <% if(obj['game']['team'][0]['scoring'][0]['quarter'][2] !== undefined) { %>
                  <td class="nbaGIQtrPts"><%=obj['game']['team'][0]['scoring'][0]['quarter'][2]['$'].points%> </td>
               <% } else { %>
                  <td>0</td>
               <% } %>
               <% if(obj['game']['team'][0]['scoring'][0]['quarter'][3] !== undefined) { %>
                  <td class="nbaGIQtrPts"><%=obj['game']['team'][0]['scoring'][0]['quarter'][3]['$'].points%> </td>
               <% } else { %>
                  <td>0</td>
               <% } %>
               <td class="nbaGIQtrPtsLast"><%=obj['game']['team'][0]['$'].points%> </td>
            </tr>
            <tr>
               <td class="nbaGIQtrStatus">
                  <div id="nbaGIQtrIndic" style="width:100%;height:9px;background:#cc0000;"></div>
               </td>
               <td class="nbaGIQtrStatus">
                  <div id="nbaGIQtrIndic" style="width:100%;height:9px;background:#cc0000;"></div>
               </td>
               <td class="nbaGIQtrStatus">
                  <div id="nbaGIQtrIndic" style="width:100%;height:9px;background:#cc0000;"></div>
               </td>
               <td class="nbaGIQtrStatus">
                  <div id="nbaGIQtrIndic" style="width:100%;height:9px;background:#cc0000;"></div>
               </td>
               <td class="nbaGIQtrStatus">
                  <div id="nbaGIQtrIndic" style="background: rgb(204, 204, 204) none repeat scroll 0% 0%; width: 100%; height: 9px; -moz-background-clip: border; -moz-background-origin: padding; -moz-background-inline-policy: continuous;"></div>
               </td>
            </tr>
             <tr>
               <% if(obj['game']['team'][1]['scoring'][0]['quarter'][0] !== undefined ) { %>
                  <td class="nbaGIQtrPts"><%=obj['game']['team'][1]['scoring'][0]['quarter'][0]['$'].points%> </td>
               <% } else { %>
                  <td>0</td>
               <% } %>
               <% if(obj['game']['team'][1]['scoring'][0]['quarter'][1] !== undefined) { %>
                  <td class="nbaGIQtrPts"><%=obj['game']['team'][1]['scoring'][0]['quarter'][1]['$'].points%> </td>
               <% } else { %>
                  <td>0</td>
               <% } %>
               <% if(obj['game']['team'][1]['scoring'][0]['quarter'][2] !== undefined) { %>
                  <td class="nbaGIQtrPts"><%=obj['game']['team'][1]['scoring'][0]['quarter'][2]['$'].points%> </td>
               <% } else { %>
                  <td>0</td>
               <% } %>
               <% if(obj['game']['team'][1]['scoring'][0]['quarter'][3] !== undefined) { %>
                  <td class="nbaGIQtrPts"><%=obj['game']['team'][1]['scoring'][0]['quarter'][3]['$'].points%> </td>
               <% } else { %>
                  <td>0</td>
               <% } %>
               <td class="nbaGIQtrPtsLast"><%=obj['game']['team'][1]['$'].points%> </td>
            </tr>
         </table>
         </div>
</div>
</div>


